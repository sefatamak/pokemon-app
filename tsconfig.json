{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"],
      "@api/*": ["api/*"],
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"],
      "@hooks/*": ["hooks/*"],
      "@lib/*": ["lib/*"],
      "@screens/*": ["screens/*"],
      "@store/*": ["store/*"],
      "@theme/*": ["theme/*"],
      "@types/*": ["types/*"]
    }
  }
}
